<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="#ff303d42"
                style="@style/MQMatchWrap.Horizontal"
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.meiqia.meiqiasdk.widget.MQImageView
        android:id="@id/photo_iv"
        android:scaleType="center"
        app:mq_iv_isSquare="true"
        style="@style/MQMatchWrap"/>

    <TextView
        android:textSize="@dimen/g7"
        android:textColor="#ffadb2bb"
        android:gravity="center"
        android:id="@id/tip_tv"
        android:layout_marginBottom="@dimen/g0"
        android:text="@string/w1"
        android:layout_alignBottom="@id/photo_iv"
        style="@style/MQMatchWrap"/>

    <ImageView
        android:id="@id/flag_iv"
        android:paddingLeft="@dimen/fz"
        android:paddingTop="@dimen/fx"
        android:paddingRight="@dimen/fx"
        android:paddingBottom="@dimen/fz"
        android:clickable="true"
        android:src="@drawable/abt"
        android:layout_alignParentRight="true"
        style="@style/MQWrapWrap"/>
</RelativeLayout>